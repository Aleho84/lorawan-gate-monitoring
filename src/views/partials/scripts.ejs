<script>
  const timeStamp = function(dateTime) {
    try {
      const newDate = new Date(dateTime);
      const separadorDia = '-';
      const separadorHora = ':';

      const año = newDate.getFullYear();
      const mes = ((newDate.getMonth() + 1) < 10) ? '0' + (newDate.getMonth() + 1) : (newDate.getMonth() + 1);
      const dia = (newDate.getDate() < 10) ? '0' + newDate.getDate() : newDate.getDate();
      // const hora = newDate.getHours();
      // const minuto = newDate.getMinutes();

      // return año + separadorDia + mes + separadorDia + dia + ' ' + hora + separadorHora + minuto;
      return año + separadorDia + mes + separadorDia + dia;
    } catch (error) {
      return {
        error: `${error}`
      };
    };
  };
</script>